{
  "name": "frontend-project-template",
  "version": "1.0.0",
  "description": "A small framework-agnostic project template for front-end web apps and sites.",
  "keywords": [
    "template",
    "boilerplate",
    "js",
    "javascript",
    "gulp",
    "browserify"
  ],
  "main": "src/index.js",
  "scripts": {
    "start": "gulp",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ianmcgregor/frontend-project-template"
  },
  "author": "ianmcgregor",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "lodash": "^4.0.1",
    "normalize.css": "^3.0.3",
    "suitcss-base": "^1.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta10",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "browserify-shim": "^3.8.12",
    "chai": "^3.5.0",
    "chalk": "^1.1.1",
    "cssnano": "^3.4.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-changed": "^1.3.0",
    "gulp-debug": "^2.1.2",
    "gulp-eslint": "^2.0.0",
    "gulp-fluent-ffmpeg": "^1.0.2",
    "gulp-if": "^2.0.0",
    "gulp-image-resize": "^0.7.1",
    "gulp-imagemin": "^2.4.0",
    "gulp-plumber": "^1.0.1",
    "gulp-postcss": "^6.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-strip-debug": "^1.1.0",
    "gulp-template": "^3.1.0",
    "gulp-uglify": "^1.5.1",
    "imagemin-webp": "^3.1.0",
    "karma": "^0.13.19",
    "karma-browserify": "^5.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "mocha": "^2.4.5",
    "postcss-calc": "^5.2.0",
    "postcss-custom-media": "^5.0.0",
    "postcss-custom-properties": "^5.0.0",
    "postcss-easings": "^0.3.0",
    "postcss-import": "^8.0.2",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^1.3.0",
    "postcss-url": "^5.1.0",
    "stylelint": "^4.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "yargs": "^3.32.0"
  },
  "vendor": {
    "lodash": "lodash"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "browser": {},
  "browserify-shim": {},
  "browserify": {
    "transform": [
      "browserify-shim",
      "babelify"
    ]
  },
  "paths": {
    "audio": {
      "entry": "src/components/**/*.{wav,aiff}",
      "dest": "dist/audio"
    },
    "connect": {
      "dir": "dist/",
      "files": [
        "dist/**/*"
      ]
    },
    "html": {
      "entry": "src/index.html",
      "dest": "dist"
    },
    "images": {
      "entry": "src/components/**/*.{png,jpg,svg,json}",
      "dest": "dist/img"
    },
    "scripts": {
      "entry": "src/index.js",
      "dest": "dist/js",
      "modernizr": {
        "entry": "src/utils/modernizr/modernizr.js",
        "dest": "dist/js"
      },
      "lint": [
        "src/**/*.js",
        "task/**/*.js",
        "!src/utils/modernizr/modernizr.js"
      ]
    },
    "static": {
      "entry": [
        "src/robots.txt",
        "src/favicon.ico"
      ],
      "dest": "dist"
    },
    "styles": {
      "entry": "src/index.css",
      "dest": "dist/css/",
      "watch": [
        "src/**/*.css"
      ],
      "lint": [
        "src/**/*.css"
      ]
    }
  }
}
