{
  "name": "frontend-project-template",
  "version": "0.0.3",
  "description": "A small framework-agnostic project template for front end web apps or sites using Gulp and Browserify.",
  "keywords": [
    "template",
    "boilerplate",
    "js",
    "javascript",
    "gulp",
    "browserify"
  ],
  "main": "src/js/index.js",
  "scripts": {
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ianmcgregor/frontend-project-template"
  },
  "author": "ianmcgregor",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "lodash": "^3.10.1",
    "normalize.css": "^3.0.3",
    "suitcss-base": "^1.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.2",
    "babel-core": "^6.3.15",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "browser-sync": "^2.10.0",
    "browserify": "^12.0.1",
    "browserify-shim": "^3.8.11",
    "chai": "^3.4.1",
    "chalk": "^1.1.1",
    "cssnano": "^3.4.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-eslint": "^1.1.1",
    "gulp-if": "^2.0.0",
    "gulp-image-resize": "^0.7.1",
    "gulp-imagemin": "^2.4.0",
    "gulp-plumber": "^1.0.1",
    "gulp-postcss": "^6.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-strip-debug": "^1.1.0",
    "gulp-uglify": "^1.5.1",
    "imagemin-webp": "^3.1.0",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "mocha": "^2.3.4",
    "postcss-calc": "^5.0.0",
    "postcss-custom-media": "^5.0.0",
    "postcss-custom-properties": "^5.0.0",
    "postcss-easings": "^0.3.0",
    "postcss-import": "^7.1.3",
    "postcss-nested": "^1.0.0",
    "postcss-url": "^5.0.2",
    "stylelint": "^3.1.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.6.1",
    "yargs": "^3.31.0"
  },
  "vendor": {},
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "browser": {},
  "browserify-shim": {},
  "browserify": {
    "transform": [
      "browserify-shim",
      "babelify"
    ]
  },
  "paths": {
    "connect": {
      "dir": "dist/",
      "files": [
        "dist/js/bundle.js",
        "dist/js/bundle.min.js",
        "dist/css/styles.css",
        "dist/index.html"
      ]
    },
    "scripts": {
      "entry": "src/index.js",
      "dest": "dist/js/",
      "bundle": "bundle.js",
      "lint": [
        "src/index.js",
        "src/js/**/*.js",
        "src/components/**/*.js",
        "task/**/*.js",
        "!src/js/modernizr/modernizr.js"
      ]
    },
    "styles": {
      "entry": "src/index.css",
      "dest": "dist/css/",
      "bundle": "styles.css",
      "watch": [
        "src/index.css",
        "src/css/**/*.css",
        "src/components/**/*.css"
      ],
      "lint": [
        "src/index.css",
        "src/css/**/*.css",
        "src/components/**/*.css"
      ]
    },
    "images": {
      "entry": "src/img/png",
      "dest": "dist/img"
    },
    "static": {
      "items": [{
        "entry": [
          "src/index.html",
          "src/robots.txt",
          "src/favicon.ico"
        ],
        "dest": "dist"
      }, {
        "entry": "src/js/modernizr/modernizr.js",
        "dest": "dist/js/modernizr"
      }],
      "watch": [
        "src/index.html",
        "src/robots.txt",
        "src/favicon.ico",
        "src/js/modernizr/modernizr.js"
      ]
    }
  }
}
